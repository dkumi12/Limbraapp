{
  "name": "limbraapp",
  "private": true,
  "version": "1.0.0",
  "description": "AI-Powered Personalized Warm-Up & Stretching App",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:watch": "vitest",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "clean": "rimraf dist dev-dist coverage .nyc_output",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "start": "npm run dev"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "lottie-react": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.10",
    "typescript": "^5.9.2",
    "vite": "^5.2.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^0.19.2",
    "vitest": "^1.6.1",
    "workbox-window": "^7.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "material-ui",
    "wellness",
    "stretching",
    "pwa",
    "ai-powered",
    "fitness"
  ],
  "homepage": "https://dkumi12.github.io/Limbraapp",
  "repository": {
    "type": "git",
    "url": "https://github.com/dkumi12/Limbraapp.git"
  },
  "bugs": {
    "url": "https://github.com/dkumi12/Limbraapp/issues"
  },
  "author": {
    "name": "dkumi12",
    "url": "https://github.com/dkumi12"
  },
  "license": "MIT"
}
